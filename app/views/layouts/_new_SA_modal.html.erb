<!-- New Modal -->
<div class="modal fade" id="newSAModal" tabindex="-1" role="dialog" aria-labelledby="newSAModalLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="newSAModalLabel">Create a New Storage Area</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <%= render 'storage_areas/create' %>
    </div>
  </div>
</div>